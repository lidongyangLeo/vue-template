{
  "name": "vue-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint:es": "eslint src/**/*.{ts,vue}",
    "lint:style": "stylelint src/**/*.vue",
    "spellcheck": "cspell lint --dot --gitignore --color --cache src/**/*.{ts,js,vue,md,json,yaml,yml}",
    "prepare": "husky",
    "commit": "git-cz",
    "test": "vitest"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "vue": "3.4.31",
    "vue-router": "4.4.0",
    "pinia": "2.1.7"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "typescript": "5.5.3",
    "vite": "5.3.3",
    "vue-tsc": "2.0.26",
    "eslint": "9.7.0",
    "@eslint/js": "9.7.0",
    "typescript-eslint": "7.16.0",
    "eslint-plugin-vue": "9.27.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "stylelint": "16.7.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "cspell": "8.10.4",
    "husky": "9.0.11",
    "commitlint": "19.3.0",
    "commitizen": "4.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "cz-git": "1.9.3",
    "vitest": "2.0.2"
  }
}
